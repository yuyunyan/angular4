{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "azimuth"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "web.config"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "az",
       "styles": [
        "../node_modules/bootstrap/scss/bootstrap.scss",
        "../node_modules/@fortawesome/fontawesome-free/css/all.css",
        "../node_modules/socicon/css/socicon.css",
        "../node_modules/metrojs/release/MetroJs.Full/MetroJs.css",
        "styles.scss",
        "../node_modules/ag-grid/dist/styles/ag-grid.css",
        "../node_modules/ag-grid/dist/styles/theme-fresh.css",
        "../node_modules/sweetalert2/dist/sweetalert2.min.css",
        "../node_modules/jquery-timepicker/jquery.timepicker.css",
        "../node_modules/angular2-busy/build/style/busy.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/tether/dist/js/tether.min.js",
        "../node_modules/popper.js/dist/umd/popper.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "../node_modules/skycons/skycons.js",
        "../node_modules/jquery-knob/dist/jquery.knob.min.js",
        "../node_modules/d3/d3.min.js",
        "../node_modules/topojson/dist/topojson.min.js",
        "../node_modules/datamaps/dist/datamaps.all.hires.min.js",
        "../node_modules/moment/moment.js",
        "../node_modules/fullcalendar/dist/fullcalendar.min.js",
        "../node_modules/jquery-ui-dist/jquery-ui.min.js",
        "../node_modules/dropzone/dist/min/dropzone.min.js",
        "../node_modules/sweetalert2/dist/sweetalert2.min.js",
        "../node_modules/jquery-timepicker/jquery.timepicker.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts",
        "ci": "environments/environment.ci.ts",
        "test": "environments/environment.test.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {},
    "serve": {
      "port": 4200
    }
  }
}
